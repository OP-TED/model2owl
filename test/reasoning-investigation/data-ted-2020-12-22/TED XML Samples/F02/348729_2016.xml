                    <NO_OPTIONS/>
                    <NO_EU_PROGR_RELATED/>
                    <INFO_ADD>
                        <P>Dynamisk indkøbssystem.</P>
                    </INFO_ADD>
                </OBJECT_DESCR>
            </OBJECT_CONTRACT>
