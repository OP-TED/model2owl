            <epo:hasDescription xml:lang="de">
                    &lt;P&gt;Unterstützung bei der Entwicklung von harmonisierten europäischen
                        Buchhaltungsstandards für den öffentlichen Sektor und von Analyseberichten
                        zur Unterstützung der Umsetzung von EPSAS.&lt;/P&gt;
                </epo:hasDescription>
            <epo:hasDescription xml:lang="ro">
                    &lt;P&gt;Asistență pentru elaborarea de standarde armonizate de contabilitate pentru
                        sectorul public european (EPSAS) și de rapoarte de analiză pentru
                        sprijinirea implementării acestora.&lt;/P&gt;
                </epo:hasDescription>
            <epo:hasDescription xml:lang="lt">
                    &lt;P&gt;Parama rengiant suderintus Europos viešojo sektoriaus apskaitos standartus ir
                        analizės ataskaitas siekiant paremti EPSAS įgyvendinimą.&lt;/P&gt;
                </epo:hasDescription>
            <epo:hasDescription xml:lang="cs">
                    &lt;P&gt;Podpora tvorby harmonizovaných účetních norem evropského veřejného sektoru a
                        zprávy o výsledku analýzy na podporu zavádění EPSAS.&lt;/P&gt;
                </epo:hasDescription>
            <epo:specifies rdf:resource="http://data.europa.eu/a4g/resource/2016/s-236-429421#Lot07522120-e8f5-47bd-9a42-054d99eaff91"/>
