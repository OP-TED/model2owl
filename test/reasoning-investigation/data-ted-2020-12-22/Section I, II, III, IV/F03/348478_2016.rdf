            <epo:hasTitle xml:lang="lv">
                    &lt;P&gt;Ceļojumu pārvaldības pakalpojumi Eiropas uzraudzības iestādēm.&lt;/P&gt;
                </epo:hasTitle>
            <epo:hasTotalValue>
              <epo:Value rdf:about="http://data.europa.eu/a4g/resource/2016/s-194-348478#ValueTotalb22c61da-39ef-4a1b-80f3-973aaf533965">
