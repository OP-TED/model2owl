            <epo:hasTitle xml:lang="en">
                    &lt;P&gt;Fire prevention at the workplace.&lt;/P&gt;
                </epo:hasTitle>
            <epo:hasTitle xml:lang="ga">
                    &lt;P&gt;Cosc dóiteáin ag an áit oibre.&lt;/P&gt;
                </epo:hasTitle>
