      <epo:Lot rdf:about="http://data.europa.eu/a4g/resource/2020/s-001-000956#Lot1f1086f1-6daa-4bf6-ad3c-7be465099341">
        <epo:hasID>
          <epo:Identifier rdf:about="http://data.europa.eu/a4g/resource/2020/s-001-000956#LotId25330019-3e4e-41b3-af06-4c00cb8f684b">
