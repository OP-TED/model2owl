              </epo:ContractTerm>
            </epo:has>
            <epo:has>
              <epo:Purpose rdf:about="http://data.europa.eu/a4g/resource/2018/s-134-306800#PurposeAdditional73fee7ad-2696-4fc3-8730-7931acf577c3">
