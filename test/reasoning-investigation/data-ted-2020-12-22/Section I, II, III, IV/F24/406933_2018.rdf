            <epo:hasPerformanceConditions xml:lang="de">
                    &lt;P&gt;Kriterien gemäß Auftragsunterlagen:&lt;/P&gt;
                    &lt;P&gt;1) Der Konzessionsvertrag&lt;/P&gt;
                    &lt;P&gt;2) Technische Bieterangaben&lt;/P&gt;
                    &lt;P&gt;3) Leistungsverzeichnis Teil I&lt;/P&gt;
                    &lt;P&gt;4) Leistungsverzeichnis Teil II&lt;/P&gt;
                    &lt;P&gt;5) Technische Angaben des Bieters&lt;/P&gt;
                </epo:hasPerformanceConditions>
          </epo:ContractTerm>
        </reg2015:applies>
