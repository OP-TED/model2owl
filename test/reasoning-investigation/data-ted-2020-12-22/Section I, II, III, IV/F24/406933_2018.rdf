        </reg2015:applies>
        <reg2015:applies>
          <epo:SelectionCriterion rdf:about="http://data.europa.eu/a4g/resource/2018/s-179-406933#SelectionCriterionTechnical8e7df0f5-dbdc-482f-bda2-ba995034bc63">
            <epo:hasSelectionCriterionType rdf:resource="http://publications.europa.eu/resource/authority/selection-criterion/tp-abil"/>
            <epo:hasDescription xml:lang="de">
                    &lt;P&gt;Einrichtung und Betrieb von mind. 2 Alarmübertragungsanlagen für
                        Brandmeldungen mit mind. 200 Übertragungseinheiten. Betrieb einer
                        Sicherheitszentrale/Haupt-Clearingstelle gemäß den Kriterien nach DIN EN VDE
                        50518.&lt;/P&gt;
                    &lt;P&gt;Zertifizierung des Bieters für die Einrichtung und den Betrieb einer
                        Alarmübertragungsanlage für Brandmeldungen gemäß VDS und DIN EN 14675.&lt;/P&gt;
                </epo:hasDescription>
          </epo:SelectionCriterion>
        </reg2015:applies>
        <reg2015:hasPerformingStaffQualificationInformation>true</reg2015:hasPerformingStaffQualificationInformation>
        <reg2015:applies>
          <epo:SelectionCriterion rdf:about="http://data.europa.eu/a4g/resource/2018/s-179-406933#SelectionCriterionSuitability800ae4ff-f965-4bde-aa58-3986d1a25fa8">
            <epo:hasSelectionCriterionType rdf:resource="http://publications.europa.eu/resource/authority/selection-criterion/sui-act"/>
            <epo:hasDescription xml:lang="de">
                    &lt;P&gt;Nachweis über die Eintragung in ein Berufs- oder Handelsregister. Nachweis
                        der Gewerbeanmeldung der zuständigen örtlichen Stelle am Sitz des
                        Bieters&lt;/P&gt;
                </epo:hasDescription>
          </epo:SelectionCriterion>
        </reg2015:applies>
        <epo:hasDescription xml:lang="de">
                    &lt;P&gt;Vergabe von bis zu 2 Konzessionen für die Einrichtung, Unterhaltung und
                        Betrieb einer Alarmübertragungsanlage für Brandmeldungen im
                        Hochsauerlandkreis&lt;/P&gt;
                </epo:hasDescription>
        <epo:hasTitle xml:lang="de">
                    &lt;P&gt;Konzessionsverträge für die Einrichtung u.d. Betrieb einer Übertragungsanlage
                        f. Brandmeldungen im Hochsauerlandkreis. Der Hochsauerlandkreis behält sich
                        vor bis zu 2 Konzessionsverträge abzuschließen.&lt;/P&gt;
                </epo:hasTitle>
        <reg2015:applies>
          <epo:SelectionCriterion rdf:about="http://data.europa.eu/a4g/resource/2018/s-179-406933#SelectionCriterionEconomic68746d51-91ad-4e89-a058-0ed0e6d5801d">
            <epo:hasSelectionCriterionType rdf:resource="http://publications.europa.eu/resource/authority/selection-criterion/ef-stand"/>
            <epo:hasDescription xml:lang="de">
                    &lt;P&gt;Nachweis von Referenzen für die Einrichtung und Betrieb von mind. 2
                        Alarmübertragungsanlagen für Brandmeldungen mit mind. 200
                        Übertragungseinheiten innerhalb der letzten 5 Jahre&lt;/P&gt;
                </epo:hasDescription>
          </epo:SelectionCriterion>
        </reg2015:applies>
