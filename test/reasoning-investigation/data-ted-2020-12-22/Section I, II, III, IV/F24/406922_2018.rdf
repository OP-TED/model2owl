    <epo:hasTitle xml:lang="en">
                    &lt;P&gt;Opportunity for 50Mw Battery Storage Development — (adjacent to Morrison
                        Busty Annfield Plain County Durham)&lt;/P&gt;
                </epo:hasTitle>
    <epo:uses>
      <epo:AccessTerm rdf:about="http://data.europa.eu/a4g/resource/2018/s-179-406922#AccessTermsFulla98148cf-3ecf-481b-bfc4-11d361b396bc">
