    <reg2015:providesAdditionalInformationAboutProcurementProcedure>
      <reg2015:Procedure rdf:about="http://data.europa.eu/a4g/extension/resource/2016/s-195-351799#Procedure74749cc8-eba4-4fa1-9133-966c646eaf6f">
