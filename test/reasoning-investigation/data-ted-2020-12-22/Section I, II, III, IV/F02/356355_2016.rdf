    <epo:hasTitle xml:lang="pt">
                    &lt;P&gt;16.CAT.OP.187: Apoio ao desenvolvimento e execução de exercícios de
                        planeamento operacional cibernético ao nível do Quartel-General da
                        Força/Quartel-General de Operações.&lt;/P&gt;
                </epo:hasTitle>
    <reg2015:hasCriteriaInProcurementDocument>
      <epo:SelectionCriterion rdf:about="http://data.europa.eu/a4g/resource/2016/s-198-356355#SelectionCriterionEconomic397a97cb-2b94-442c-9dbf-6fe27bf05f1c">
