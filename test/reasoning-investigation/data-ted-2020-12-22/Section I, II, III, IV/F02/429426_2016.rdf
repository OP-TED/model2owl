            <epo:hasTitle xml:lang="da">
                    &lt;P&gt;Vedligeholdelse og bistand i forbindelse med driften af almindeligt
                        sikkerhedsudstyr på Europa-Parlamentets arbejdssted i Strasbourg.&lt;/P&gt;
                </epo:hasTitle>
            <epo:hasTitle xml:lang="pt">
                    &lt;P&gt;Manutenção e assistência à exploração dos equipamentos de segurança geral nas
                        instalações do Parlamento Europeu em Estrasburgo.&lt;/P&gt;
                </epo:hasTitle>
            <epo:uses>
              <epo:AccessTerm rdf:about="http://data.europa.eu/a4g/resource/2016/s-236-429426#AccessTermsFull1bf9e680-ac3e-454d-b8f3-df244355f993">
