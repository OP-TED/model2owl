      <reg2015:Procedure rdf:about="http://data.europa.eu/a4g/extension/resource/2018/s-175-397711#Procedurecc1ba256-814a-4ea9-89c8-b5f17413baed">
        <epo:hasOverallPurpose>
          <epo:Purpose rdf:about="http://data.europa.eu/a4g/resource/2018/s-175-397711#Purposea56fcdcd-924b-4a06-a656-67cd7687b328">
