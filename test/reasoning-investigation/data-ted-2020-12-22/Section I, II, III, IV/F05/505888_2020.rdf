                <epo:hasTitle xml:lang="pl">&lt;P&gt;SUG eksploatowane przez PSE S.A. ZKO w Katowicach&lt;/P&gt;</epo:hasTitle>
                <epo:isSpecifiedIn rdf:resource="http://data.europa.eu/a4g/extension/resource/2020/s-207-505888#Procedure869cdbf7-c61d-424f-a017-9231864169e7"/>
