    <x:scenario label="Scenario for testing template with match 'element[@xmi:type = 'uml:Class']/attributes/attribute">
        <x:context href="../../testData/ePO-CM-v2.0.1-2020-03-27_test.eap.xmi" select="/xmi:XMI/xmi:Extension[1]/elements[1]/element[55]/attributes[1]/attribute[2]"/>
        <x:expect label="there is 2 rdf:Description" test="boolean(rdf:Description) "/>
        <x:expect label="there is a rdfs:domain" test="boolean(rdf:Description/rdfs:domain)"/>
        <x:expect label="there is a owl:DatatypeProperty" test="boolean(owl:DatatypeProperty)"/>
        <x:expect label="there is no owl:ObjectProperty" test="not(boolean(owl:ObjectProperty))"/>
        <x:expect label="there is owl:Class" test="boolean(owl:Class)"/>
        <x:expect label="there is a owl:Restriction" test="boolean(owl:Class/rdfs:subClassOf/owl:Restriction) "/>
        <x:expect label="there is a owl:onProperty" test="boolean(owl:Class/rdfs:subClassOf/owl:Restriction/owl:onProperty)"/>
        <x:expect label="there is a rdf:type" test="boolean(rdf:Description/rdf:type)"/>
    </x:scenario>
    
    <x:scenario label="Scenario for attribute domain">
        <x:call template="attributeDomain">
            <x:param name="attribute" href="../../testData/ePO-CM-v2.0.1-2020-03-27_test.eap.xmi" select="/xmi:XMI/xmi:Extension[1]/elements[1]/element[290]/attributes[1]/attribute[6]"/>
        </x:call>   
        <x:expect label="there is a rdf:Description" test="boolean(rdf:Description) "/>
        <x:expect label="there is a rdfs:domain" test="boolean(rdf:Description/rdfs:domain)"/>
        <x:expect label="there is a correct parent class URI" test="rdf:Description/rdfs:domain/@rdf:resource = 'http://data.europa.eu/a4g/ontology#Identifier'"/>
    </x:scenario>
    
    <x:scenario label="Scenario for attribute range - Object property">
