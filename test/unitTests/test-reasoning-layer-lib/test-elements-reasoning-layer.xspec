    <x:scenario label="Scenario for attribute multiplicity min value equals to 1 and multiplicity max value equals to 1 ">
        <x:call template="attributeMultiplicityOne">
            <x:param name="attribute" href="../../testData/ePO-CM-v2.0.1-2020-03-27_test.eap.xmi" select="/xmi:XMI/xmi:Extension[1]/elements[1]/element[140]/attributes[1]/attribute[3]"/>
        </x:call>   
