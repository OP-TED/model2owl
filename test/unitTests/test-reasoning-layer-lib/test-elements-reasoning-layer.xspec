        </x:call>   
        <x:expect label="there is a owl:ObjectProperty" test="boolean(owl:ObjectProperty) "/>
        <x:expect label="there is a rdfs:range" test="boolean(owl:ObjectProperty/rdfs:range)"/>
        <x:expect label="there is a correct class URI " test="owl:ObjectProperty/rdfs:range/@rdf:resource = 'http://data.europa.eu/a4g/ontology#AccessTerms'"/>
    </x:scenario>
    
    <x:scenario label="Scenario for attribute range - Datatype property">
