            <x:param name="namespaceDefinitions" href="../testData/namespaces.xml" select="/"></x:param>
        </x:call>
        <x:expect label="ccev base name">http://data.europa.eu/m8g/</x:expect>
    </x:scenario>
    
    <x:scenario label="Scenario (negative) for testing function getNamespaceValues ">
