            <x:param name="namespaceDefinitions" href="../testData/namespaces.xml" select="/"></x:param>
        </x:call>
        <x:expect label="return nothing" test=". = ''"/>
    </x:scenario>

    <x:scenario label="Scenario for testing function getUmlDataTypeValues">
