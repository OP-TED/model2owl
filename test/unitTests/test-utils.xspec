    <x:scenario label="Scenario for testing function getXsdRdfDataTypeValues">
        <x:call template="getXsdRdfDataTypeValues">
            <x:param name="qname">xsd:gYearMonth</x:param>
            <x:param name="dataTypesDefinitions" href="../testData/xsdAndRdfDataTypes.xml" select="/"></x:param>
        </x:call>
        <x:expect label="data-type">xsd:gYearMonth</x:expect>
    </x:scenario>

    <x:scenario label="Scenario (positive) for testing function getNamespaceValues ">
        <x:call template="getNamespaceValues">
