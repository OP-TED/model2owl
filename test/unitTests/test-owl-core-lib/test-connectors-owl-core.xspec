        <x:expect label="result"
            test="/owl:ObjectProperty/@rdf:about='http://data.europa.eu/a4g/ontology#has'"/>
        <x:expect label="result"
            test="/owl:ObjectProperty/rdfs:subPropertyOf/@rdf:resource='http://data.europa.eu/a4g/ontology#hasPersonRole'"
        />
    </x:scenario>
    
