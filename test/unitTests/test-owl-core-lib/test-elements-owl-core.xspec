        <x:context href="../../testData/ePO-CM_v2.0.1_test.eap.xmi"
            select="/xmi:XMI/xmi:Extension[1]/elements[1]/element[132]/attributes[1]/attribute[2]"/>
        <x:expect label="there is no skos:Concept" select="()"/>
