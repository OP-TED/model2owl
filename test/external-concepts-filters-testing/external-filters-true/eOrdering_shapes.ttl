        ord-shape:epo-ord-TaxInformation-epo-cat-hasTaxScheme,
        ord-shape:epo-ord-TaxInformation-epo-isCalculatedOn ;
    sh:targetClass :TaxInformation .

ord-shape:epo-Contract-epo-ord-hasTaxInformation a sh:PropertyShape ;
