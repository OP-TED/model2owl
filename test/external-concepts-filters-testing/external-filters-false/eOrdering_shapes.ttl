core-shape:epo-ord-AllowanceChargeInformation a sh:NodeShape ;
    rdfs:label "Allowance charge information" ;
    rdfs:comment "Information about discounts, taxes, duties and fees imposed. WG approval 16/05/2023" ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:property core-shape:epo-ord-AllowanceChargeInformation-epo-cat-hasAmount,
        core-shape:epo-ord-AllowanceChargeInformation-epo-cat-hasPercentage,
        core-shape:epo-ord-AllowanceChargeInformation-epo-ful-hasAllowanceChargeReason,
        core-shape:epo-ord-AllowanceChargeInformation-epo-ful-hasAllowanceChargeReasonDescription,
        core-shape:epo-ord-AllowanceChargeInformation-epo-isCalculatedOn,
        core-shape:epo-ord-AllowanceChargeInformation-epo-ord-hasTaxInformation ;
    sh:targetClass :AllowanceChargeInformation .

core-shape:epo-ord-AllowanceInformation a sh:NodeShape ;
    rdfs:label "Allowance information" ;
    rdfs:comment "Information about the discounts imposed. WG approval 16/05/2023" ;
