<?xml version="1.0" encoding="UTF-8"?>
<x:description query="x-urn:test:mirror" query-at="../../mirror.xqm" stylesheet="../../mirror.xsl"
	xmlns:mirror="x-urn:test:mirror" xmlns:x="http://www.jenitennison.com/xslt/xspec"
	xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<x:scenario label="In a failure report HTML">
		<x:call function="mirror:true" />
		<x:expect select="xs:decimal(1)">
			<x:label>[Expected Result] must represent xs:decimal(1) by "1.0" (numeric literal of
				decimal)</x:label>
		</x:expect>
		<x:expect select="xs:double(1)">
			<x:label>[Expected Result] must represent xs:double(1) by "1.0e0" (numeric literal of
				double)</x:label>
		</x:expect>
		<x:expect select="xs:integer(1)">
			<x:label>[Expected Result] must represent xs:integer(1) by "1" (numeric literal of
				integer)</x:label>
		</x:expect>
	</x:scenario>
</x:description>
