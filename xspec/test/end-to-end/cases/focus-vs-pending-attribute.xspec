<x:description query="x-urn:test:do-nothing" query-at="do-nothing.xqm" schematron="do-nothing.sch"
	stylesheet="do-nothing.xsl" xml:base="../../" xmlns:x="http://www.jenitennison.com/xslt/xspec">

	<x:scenario label="Expect" shared="yes">
		<x:expect label="x:expect[empty(@pending)]" test="true()" />
		<x:expect label="x:expect[@pending]" pending="x:expect/@pending" test="true()" />
	</x:scenario>

	<x:scenario label="/Neutral">
		<x:call function="true" />
		<x:like label="Expect" />
	</x:scenario>

	<x:scenario label="/Neutral">
		<x:call function="true" />
		<x:like label="Expect" />

		<x:scenario label="/Neutral">
			<x:like label="Expect" />
		</x:scenario>

		<x:scenario focus="Focus #1" label="/Focused #1">
			<x:like label="Expect" />

			<x:scenario label="/Neutral">
				<x:like label="Expect" />
			</x:scenario>

			<x:scenario label="/Neutral">
				<x:like label="Expect" />

				<x:scenario label="/Neutral">
					<x:like label="Expect" />
				</x:scenario>

				<x:scenario label="/Pending #1" pending="Pending #1">
					<x:like label="Expect" />

					<x:scenario label="/Neutral">
						<x:like label="Expect" />
					</x:scenario>

					<x:scenario label="/Neutral">
						<x:like label="Expect" />

						<x:scenario label="/Neutral">
							<x:like label="Expect" />
						</x:scenario>

						<x:scenario focus="Focus #2" label="/Focused #2">
							<x:like label="Expect" />

							<x:scenario label="/Neutral">
								<x:like label="Expect" />
							</x:scenario>

							<x:scenario label="/Neutral">
								<x:like label="Expect" />

								<x:scenario label="/Neutral">
									<x:like label="Expect" />
								</x:scenario>

								<x:scenario label="/Pending #2" pending="Pending #2">
									<x:like label="Expect" />

									<x:scenario label="/Neutral">
										<x:like label="Expect" />
									</x:scenario>

								</x:scenario>

							</x:scenario>

						</x:scenario>

					</x:scenario>

				</x:scenario>

			</x:scenario>

		</x:scenario>

	</x:scenario>

</x:description>
